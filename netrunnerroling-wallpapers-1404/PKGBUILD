# Maintainer:Leszek Lesner <leszek@zevenos.com>
pkgname=netrunnerroling-wallpapers-1404
pkgver=0+git20140306.8c08e6e
pkgrel=1
pkgdesc="Default Wallpapers for Netrunner 2014.04"
arch=('any')
url='https://github.com/netrunnerarch/netrunnerrolling-wallpapers'
license=('GPL')
groups=('kde')
makedepends=('git')
source=('git://github.com/netrunnerarch/netrunnerrolling-wallpapers.git#branch=2014-04')
md5sums=('SKIP')

pkgver() {
cd "$srcdir/netrunnerroling-wallpapers"
printf "0+git`date +%Y%m%d`.%s"  "$(git rev-parse --short HEAD)"
}


package() {
  cd $srcdir/netrunnerroling-wallpapers 
  cp -aR usr $pkgdir/
}
